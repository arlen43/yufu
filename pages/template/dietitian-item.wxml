<template name="dietitian-item">
  <!-- 元素图片开始 -->
  <view class="item-present">
    <image class="dietitian-img" src="{{dietitian.img}}"></image>
  </view>
  <!-- 元素图片结束 -->
  <!-- 元素详情开始 -->
  <view class="item-detail">
    <view class="item-title f34">{{dietitian.name}}</view>
    <view class="item-desc dietitian-desc f24 color-gray6">{{dietitian.description}}</view>
    <view class="item-author">
      <image src="{{dietitian.img}}"></image>
      <image src="{{dietitian.img}}" class="ml5"></image>
      <image src="{{dietitian.img}}" class="ml5"></image>
      <text class="pl5 f24 color-gray6">鱼小宝等8位朋友也选择他</text>
    </view>
    <view class="item-exta f24 color-green">
      <text class="f30 iconfont icon-collection_fill"></text>
      <text class="f30 iconfont icon-collection_fill"></text>
      <text class="f30 iconfont icon-collection_fill"></text>
      <text class="f30 iconfont icon-collection_fill"></text>
      <text class="f30 iconfont icon-collection_fill"></text>
      <text class="pl5 color-gray6">已有{{comment.count}}人评价</text>
    </view>
  </view>
  <!-- 元素详情结束 -->
  <view class="item-operate">
    <view class="button bt-green f24" catchtap="naviToShopPage">去咨询</view>
  </view>
</template>
